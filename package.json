{
    "name": "blank-line-cleaner",
    "displayName": "Even Row Remover",
    "description": "Removes even-numbered rows from documents",
    "version": "0.0.3",
    "publisher": "YourPublisherName",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/blank-line-cleaner"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:blanklinecleaner.clean"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "blanklinecleaner.clean",
            "title": "Remove Even-Numbered Rows"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.50.0",
        "@types/node": "^14.0.0",
        "typescript": "^4.0.0"
    }
}
